framework module TensorFlowLite {
   
   // without internal separation into modules
   // this is guaranteed to work in a resulting SPM binary target
   // header "TensorFlowLiteC.h"
   // header "TensorFlowLiteCMetal.h"
   // header "TensorFlowLiteCCoreML.h"
   
   // submodule modules
   module C {
      header "TensorFlowLiteC.h"
   }
   
   module CMetal {
      header "TensorFlowLiteCMetal.h"
   }
   
   module CCoreML {
      header "TensorFlowLiteCCoreML.h"
   }
   
}
